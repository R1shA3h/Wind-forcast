
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("POSTGRES_PRISMA_URL") // uses connection pooling
  directUrl = env("POSTGRES_URL_NON_POOLING") // uses a direct connection
}

model WindActual {
  id               Int      @id @default(autoincrement())
  startTime        DateTime
  generation       Int
}

model WindForecast {
  id           Int      @id @default(autoincrement())
  publishTime  DateTime
  startTime    DateTime
  generation   Int
}